{
  "name": "rhythmic-excellence-react",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://andreasonny83.github.io/rhythmic-excellence-react/",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-postcss start-js",
    "build-js": "react-scripts build",
    "config:prod": "rm build/admin/*.yml && cp public/admin/config.prod.yml build/admin/config.yml",
    "build": "npm-run-all build-postcss build-js config:prod",
    "test:react": "react-scripts test --env=jsdom",
    "test": "echo \"Error: no test specified\" && exit 0",
    "eject": "react-scripts eject",
    "build-postcss": "postcss --env=production --dir=src/ --ext=css --base=src/ src/*.pcss src/**/*.pcss",
    "watch-postcss": "postcss --dir=src/ --ext=css --verbose --base=src/ -w src/*.pcss src/**/*.pcss",
    "deploy:admin": "cd admin && now --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "deploy:admin:fromlocal": "cd auth-server && now && now alias",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^4.3.0",
    "react": "^16.4.1",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^16.4.1",
    "react-html-parser": "^2.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-burger-menu": "^0.2.7",
    "redux-devtools-extension": "^2.13.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "cssnano": "^4.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "gh-pages": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.23",
    "postcss-cli": "^5.0.1",
    "precss": "^3.1.2",
    "prettier": "1.13.7"
  }
}
